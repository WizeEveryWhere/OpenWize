<?xml version="1.0" encoding="UTF-8"?>
<tns:ParamCfg xmlns:tns="http://www.grdf.fr/smart-solutions/TU/1.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.grdf.fr/smart-solutions/TU/1.0.0/Restrictions.xsd ">  
 <RestrictionList>
    <!-- Warning : the Restriction value or Ordering are is not checked -->
     
    <!-- rtype is one of -->
    <!--     MODULO      -->
    <!--     RANGE       -->
    <!--     ENUM        -->

    <!-- rsize is one of -->
    <!--    8BITS        -->
    <!--    16BITS       -->
    <!--    32BITS       -->
    <!--    64BITS       -->
    
    <!-- Range 8 -->
    <Restriction id="01" rtype="RANGE" rsize="8BITS"> 
        20 200 
    </Restriction>
    <!-- Range 16 -->
    <Restriction id="02" rtype="RANGE" rsize="16BITS"> 
        20 2000 
    </Restriction>
    <!-- Range 32 -->
    <Restriction id="03" rtype="RANGE" rsize="32BITS"> 
        20 200000 
    </Restriction>
    <!-- Range 64 -->
    <Restriction id="04" rtype="RANGE" rsize="64BITS"> 
        20 200000000000
    </Restriction>
    
    <!-- Enum 8 -->
    <Restriction id="05" rtype="ENUM" rsize="8BITS"> 
        2 12 48 144
    </Restriction>
    <!-- Enum 16 -->
    <Restriction id="06" rtype="ENUM" rsize="16BITS">
        2 12 48 144 2000
    </Restriction>
    <!-- Enum 32 -->
    <Restriction id="07" rtype="ENUM" rsize="32BITS">
        2 12 48 144 2000 200000
    </Restriction>
    <!-- Enum 64 -->
    <Restriction id="08" rtype="ENUM" rsize="64BITS">
        2 12 48 144 2000  200000 200000000000
    </Restriction>
    
    <!-- Modulo 8 : value is limited to 255 -->
    <Restriction id="09" rtype="MODULO" rsize="8BITS">
        5
    </Restriction>
    <!-- Enum 16 : value is limited to 65535 -->
    <Restriction id="0A" rtype="MODULO" rsize="16BITS">
        50
    </Restriction>
    <!-- Enum 32 : value is limited to 4294967296 -->
    <Restriction id="0B" rtype="MODULO" rsize="32BITS">
        5000
    </Restriction>
    <!-- Enum 64 : value is limited to 1,84467440737e+19 -->
    <Restriction id="0C" rtype="MODULO" rsize="64BITS">
        50000
    </Restriction>
 </RestrictionList>

</tns:ParamCfg>
